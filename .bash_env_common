# ---------------------------------------------------------------------
# 共通環境変数
# ---------------------------------------------------------------------

# GoogleDriveまでのパス
export GDRIVE="$HOME/Google Drive"

# secret_settingsを格納しているパス
export SECRET_SETTINGS="$GDRIVE/マイドライブ/secret_settings"

# dotの実行環境指定
export DOT_ENVIRONMENT=local

# Goのプロトコルバッファーの名前空間のコンフリクトをwarningとして扱う
export GOLANG_PROTOBUF_REGISTRATION_CONFLICT="warn"

