# ---------------------------------------------------------------------
# 共通環境変数
# ---------------------------------------------------------------------

# CLIで使用したいエディタの実行ファイルのパス
export EDITOR="/usr/bin/vi"

# GoogleDriveまでのパス
export GDRIVE="$HOME/GoogleDrive"

# toolsを格納しているパス
export TOOLS="$GDRIVE/tools"

# dotの実行環境指定
export DOT_ENVIRONMENT=local
